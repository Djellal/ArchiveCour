{
  "access": "Authenticated",
  "components": [
    {
      "components": [
        {
          "components": [
            {
              "class": "col-md-12",
              "components": [
                {
                  "components": [
                    {
                      "components": [
                        {
                          "components": [
                            {
                              "class": "col-md-2",
                              "components": [
                                {
                                  "component": "num",
                                  "name": "numLabel",
                                  "style": {
                                    "width": "100%"
                                  },
                                  "text": "N°",
                                  "type": "label"
                                },
                                {
                                  "maxLength": 100,
                                  "name": "num",
                                  "style": {
                                    "display": "block",
                                    "width": "100%"
                                  },
                                  "type": "textbox",
                                  "value": "${courrier.num}",
                                  "valueType": "string"
                                },
                                {
                                  "component": "numInterne",
                                  "name": "numInterneLabel",
                                  "style": {
                                    "width": "100%"
                                  },
                                  "text": "N° Interne",
                                  "type": "label"
                                },
                                {
                                  "maxLength": 100,
                                  "name": "numInterne",
                                  "style": {
                                    "display": "block",
                                    "width": "100%"
                                  },
                                  "type": "textbox",
                                  "value": "${courrier.numInterne}",
                                  "valueType": "string"
                                }
                              ],
                              "name": "col1",
                              "type": "col"
                            },
                            {
                              "class": "col-md-2",
                              "components": [
                                {
                                  "component": "DateArrivDepart",
                                  "name": "DateArrivDepartLabel",
                                  "style": {
                                    "width": "100%"
                                  },
                                  "text": "Date Arriv Depart",
                                  "type": "label"
                                },
                                {
                                  "name": "DateArrivDepart",
                                  "style": {
                                    "display": "block",
                                    "width": "100%"
                                  },
                                  "type": "datepicker",
                                  "value": "${courrier.DateArrivDepart}",
                                  "valueType": "DateTime"
                                },
                                {
                                  "component": "DateCorresp",
                                  "name": "DateCorrespLabel",
                                  "style": {
                                    "width": "100%"
                                  },
                                  "text": "Date Corresp",
                                  "type": "label"
                                },
                                {
                                  "name": "DateCorresp",
                                  "style": {
                                    "display": "block",
                                    "width": "100%"
                                  },
                                  "type": "datepicker",
                                  "value": "${courrier.DateCorresp}",
                                  "valueType": "DateTime"
                                }
                              ],
                              "name": "col0",
                              "type": "col"
                            },
                            {
                              "class": "col-md-8",
                              "components": [
                                {
                                  "component": "Typeid",
                                  "name": "TypeidLabel",
                                  "style": {
                                    "width": "100%"
                                  },
                                  "text": "Types Courrier",
                                  "type": "label"
                                },
                                {
                                  "data": "${getTypesCourriersResult}",
                                  "name": "Typeid",
                                  "placeholder": "Choose TypesCourrier",
                                  "style": {
                                    "width": "100%"
                                  },
                                  "textProperty": "NomType",
                                  "type": "dropdown",
                                  "value": "${courrier.Typeid}",
                                  "valueProperty": "Id",
                                  "valueType": "int?"
                                },
                                {
                                  "component": "Categorid",
                                  "name": "CategoridLabel",
                                  "style": {
                                    "width": "100%"
                                  },
                                  "text": "Category",
                                  "type": "label"
                                },
                                {
                                  "data": "${getCategoriesResult}",
                                  "name": "Categorid",
                                  "placeholder": "Choose Category",
                                  "style": {
                                    "width": "100%"
                                  },
                                  "textProperty": "NomCateg",
                                  "type": "dropdown",
                                  "value": "${courrier.Categorid}",
                                  "valueProperty": "Id",
                                  "valueType": "int?"
                                }
                              ],
                              "name": "col3",
                              "type": "col"
                            }
                          ],
                          "name": "row0",
                          "type": "row"
                        },
                        {
                          "components": [
                            {
                              "class": "col-md-4",
                              "components": [
                                {
                                  "component": "Correspid",
                                  "name": "CorrespidLabel",
                                  "style": {
                                    "width": "100%"
                                  },
                                  "text": "Correspondant",
                                  "type": "label"
                                },
                                {
                                  "data": "${getCorrespondantsResult}",
                                  "name": "Correspid",
                                  "placeholder": "Choose Correspondant",
                                  "style": {
                                    "width": "100%"
                                  },
                                  "textProperty": "Nom",
                                  "type": "dropdown",
                                  "value": "${courrier.Correspid}",
                                  "valueProperty": "Id",
                                  "valueType": "int?"
                                }
                              ],
                              "name": "col2",
                              "type": "col"
                            },
                            {
                              "class": "col-md-8",
                              "components": [
                                {
                                  "component": "Correspid",
                                  "name": "label0",
                                  "style": {
                                    "width": "100%"
                                  },
                                  "text": "Observation",
                                  "type": "label"
                                },
                                {
                                  "name": "textbox0",
                                  "style": {
                                    "width": "100%"
                                  },
                                  "type": "textbox",
                                  "value": ""
                                }
                              ],
                              "name": "col4",
                              "type": "col"
                            }
                          ],
                          "name": "row1",
                          "type": "row"
                        }
                      ],
                      "name": "card0",
                      "type": "card",
                      "visible": true
                    },
                    {
                      "components": [
                        {
                          "class": "col-md-3",
                          "components": [],
                          "type": "col"
                        },
                        {
                          "class": "col-md-9",
                          "components": [],
                          "type": "col"
                        }
                      ],
                      "style": {
                        "marginBottom": "1rem"
                      },
                      "type": "row"
                    },
                    {
                      "components": [
                        {
                          "class": "col offset-sm-3",
                          "components": [
                            {
                              "buttonStyle": "primary",
                              "buttonType": "submit",
                              "icon": "save",
                              "text": "Save",
                              "type": "button"
                            },
                            {
                              "buttonStyle": "light",
                              "events": {
                                "click": [
                                  {
                                    "type": "close-popup"
                                  }
                                ]
                              },
                              "style": {
                                "marginLeft": "1rem"
                              },
                              "text": "Cancel",
                              "type": "button"
                            }
                          ],
                          "type": "col"
                        }
                      ],
                      "type": "row"
                    }
                  ],
                  "data": "${courrier}",
                  "events": {
                    "submit": [
                      {
                        "error": [
                          {
                            "detail": "Unable to create new Courrier!",
                            "severity": "error",
                            "summary": "Error",
                            "type": "notify"
                          }
                        ],
                        "name": "Appdb.createCourrier",
                        "parameters": [
                          {
                            "name": "Courrier",
                            "value": "${courrier}"
                          }
                        ],
                        "then": [
                          {
                            "result": "${courrier}",
                            "type": "close-popup"
                          }
                        ],
                        "type": "invoke"
                      }
                    ]
                  },
                  "itemType": "ArchiveCorr.Models.Appdb.Courrier",
                  "name": "form0",
                  "type": "template-form",
                  "visible": "${courrier != null}"
                }
              ],
              "type": "col"
            }
          ],
          "type": "row"
        }
      ],
      "container": "main",
      "type": "content"
    }
  ],
  "events": {
    "load": [
      {
        "name": "Appdb.getStructures",
        "parameters": [],
        "then": [
          {
            "name": "getStructuresResult",
            "type": "set",
            "value": "${result}",
            "valueType": "IEnumerable<ArchiveCorr.Models.Appdb.Structure>"
          }
        ],
        "type": "invoke"
      },
      {
        "name": "Appdb.getTypesCourriers",
        "parameters": [],
        "then": [
          {
            "name": "getTypesCourriersResult",
            "type": "set",
            "value": "${result}",
            "valueType": "IEnumerable<ArchiveCorr.Models.Appdb.TypesCourrier>"
          }
        ],
        "type": "invoke"
      },
      {
        "name": "Appdb.getCategories",
        "parameters": [],
        "then": [
          {
            "name": "getCategoriesResult",
            "type": "set",
            "value": "${result}",
            "valueType": "IEnumerable<ArchiveCorr.Models.Appdb.Category>"
          }
        ],
        "type": "invoke"
      },
      {
        "name": "Appdb.getCorrespondants",
        "parameters": [],
        "then": [
          {
            "name": "getCorrespondantsResult",
            "type": "set",
            "value": "${result}",
            "valueType": "IEnumerable<ArchiveCorr.Models.Appdb.Correspondant>"
          }
        ],
        "type": "invoke"
      },
      {
        "name": "courrier",
        "type": "set",
        "value": "new ArchiveCorr.Models.Appdb.Courrier(){}",
        "valueType": "ArchiveCorr.Models.Appdb.Courrier"
      }
    ]
  },
  "layout": "Main",
  "name": "Add Courrier"
}